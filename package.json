{
  "private": true,
  "name": "parent",
  "version": "0.0.0",
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=8.9.3"
  },
  "resolution": {
    "**/@types/node": "8.10.20"
  },
  "devDependencies": {
    "@malagu/ext-scripts": "^1.1.0",
    "@types/chai": "^4.0.1",
    "@types/chai-string": "^1.4.0",
    "@types/jsdom": "^16.2.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "8.10.20",
    "@types/sinon": "^2.3.5",
    "@types/temp": "^0.8.29",
    "@types/webdriverio": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.26.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@wdio/cli": "^5.21.0",
    "@wdio/mocha-framework": "^5.16.15",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "chai": "^4.1.0",
    "chai-string": "^1.4.0",
    "concurrently": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-no-null": "^1.0.2",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^16.2.2",
    "lerna": "^2.2.0",
    "mocha": "^6.1.0",
    "nyc": "^11.0.3",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "sinon": "^3.3.0",
    "temp": "^0.8.3",
    "tslint": "^6.1.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3",
    "typescript-eslint-language-service": "^2.1.3",
    "uuid": "^3.1.0",
    "webdriverio": "^5.18.0"
  },
  "scripts": {
    "prepare": "yarn run build:clean",
    "build": "lerna run build --scope \"@cellbang/!(example-)*\"",
    "clean": "lerna run clean",
    "build:clean": "lerna run prepare",
    "lint": "run lint",
    "lint:clean": "rimraf .eslintcache",
    "lint:oneshot": "node --max-old-space-size=4096 node_modules/eslint/bin/eslint.js --cache=true \"{dev-packages,packages,examples}/**/*.{ts,tsx}\"",
    "docs": "rimraf gh-pages/docs/next && typedoc --tsconfig configs/typedoc-tsconfig.json --options configs/typedoc.json",
    "test": "yarn test:cellbang",
    "test:cellbang": "lerna run test --scope \"@cellbang/!(example-)*\" --parallel",
    "watch": "lerna run watch --scope \"@cellbang/!(example-)*\" --parallel",
    "publish": "yarn && yarn test && lerna publish",
    "next:publish": "lerna publish --exact --canary=next --npm-tag=next --yes"
  },
  "workspaces": [
    "dev-packages/*",
    "packages/*",
    "apps/*",
    "app"
  ]
}
